<Window x:Class="Corkboard.UI.Popups.Error"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Corkboard.UI.Popups"
        mc:Ignorable="d"
        Title="Error" Height="200" Width="300">
    <Grid>
        <TextBlock x:Name="ErrorMessage" HorizontalAlignment="Left" Margin="10,49,0,0" TextWrapping="Wrap" Text="Invalid input given, please supply different input." VerticalAlignment="Top" Height="110" Width="272" TextAlignment="Center" FontSize="18"/>

    </Grid>
</Window>
